{
  "consul": "{{ .CONSUL_ADDR }}:8500",
  "preStart": "/opt/capi/bin/fetch-api-pubkey",
  "services": [
    {
      "name": "capi",
      "port": 8080,
      "health": "/usr/bin/curl --silent --show-error --output /dev/null localhost:8080",
      "poll": 1,
      "ttl": 2,
      "interfaces": ["eth0"]
    }
  ]
}
